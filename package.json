{
  "name": "pps-fe-vue-boilerplate",
  "version": "0.5.7",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open --mode serve-dev",
    "serve:mock": "vue-cli-service serve --open --mode serve-mock",
    "build": "npm run build:dev",
    "build:dev": "vue-cli-service build --mode build-dev",
    "build:prod": "vue-cli-service build --modern --mode build-prod",
    "build:test": "vue-cli-service build --mode build-test",
    "lint": "vue-cli-service lint",
    "lint:all": "eslint --ext .js,.vue --fix . && stylelint --fix \"src/**/*.vue\" \"src/**/*.less\" && markdownlint docs/*.md *.md && prettier --list-different --write \"src/**/*.{js,json,css,less,vue,md}\"",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:watch": "vue-cli-service test:unit --watch --notify --notifyMode change",
    "release": "node scripts/release.js",
    "new": "hygen new",
    "docs": "vuepress dev",
    "docs:build": "vuepress build",
    "docs:deploy": "node scripts/docs-deploy.js",
    "docs:release": "npm run docs:build & npm run docs:deploy"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "debounce-promise": "^3.1.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.0",
    "nprogress": "^0.2.0",
    "vue": "^2.5.17",
    "vue-meta": "^1.5.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vux": "^2.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@laomao800/webpack-archive-plugin": "^2.0.0",
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-plugin-unit-jest": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "@vue/eslint-config-prettier": "^3.0.3",
    "@vue/eslint-config-standard": "^3.0.3",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-component": "^1.1.1",
    "compression-webpack-plugin": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "execa": "^1.0.0",
    "express": "^4.16.3",
    "express-delay": "^0.2.0",
    "ftp-deploy": "^2.1.3",
    "hygen": "^1.6.2",
    "imagemin-lint-staged": "^0.3.0",
    "inquirer": "^6.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "markdownlint-cli": "^0.13.0",
    "semver": "^5.5.1",
    "stylelint": "^9.5.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "vue-jest": "^2.6.0",
    "vue-loader": "^14.x",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^0.14.4",
    "vux-loader": "^1.2.9"
  }
}
